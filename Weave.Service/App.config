<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="weave" type="Weave.Core.Models.WeaveConfigurationSection, Weave.Core" />
        <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    </configSections>
    <!--databaseType=SQLite, MySQL, SQLServer-->
    <weave databaseType="SQLite" 
           enableSsl="true" 
           port="8888"
           enableAdminService="true" 
           enableAdminSsl="true" 
           adminPort="8889"
           adminLogin="admin"
           adminPassword="pass"
           enableDebug="true" 
           diagPassword="pass"
           hmacPass="ewGye%^@^%Tfr"
           rijndaelPass="$dfYTHsd%^#%^"/>
    <connectionStrings></connectionStrings>
    <nlog>
        <targets>
            <target name="file"
                    type="File"
                    archiveFileName="${basedir}/Logs/Archives/log.${shortdate}.txt"
                    archiveEvery="Day"
                    maxArchiveFiles="20"
                    fileName="${basedir}/Logs/log.txt" />
        </targets>

        <rules>
            <logger name="*" minlevel="Trace" writeTo="file" />
        </rules>
    </nlog>
</configuration>